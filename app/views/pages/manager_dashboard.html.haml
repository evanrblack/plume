.container
  %h1.text-center #{@group.name}
  .row
    .col-md-3
      = quick_look(@clients.length, 'Clients', clients_path, 'success')
    .col-md-3
      = quick_look(@caregivers.length, 'Caregivers', caregivers_path, 'info')
    .col-md-3
      = quick_look(@unarranged_visits.length, 'Unarranged Visits', '/', 'warning')
    .col-md-3
      = quick_look(4, 'Late Visits', '/', 'danger')
  .row
    .col-md-3
      = quick_look(number_with_delimiter(1942), 'Hours Billed', '/', 'success')
    .col-md-3
      = quick_look("10.22%", 'Readmission Rate', '/', 'danger')
  = render partial: 'shared/calendar'
